    "import numpy as np\n",
    "import cv2 as cv\n",
    "from matplotlib import pyplot as plt\n",
    "from pathlib import Path\n",
    "\n",
    "\n",
    "img = np.zeros((11,11,3))\n",
    "\n",
    "img = cv.circle(img, (5,5), 4, (255,255,255), -1)\n",
    "\n",
    "plt.imshow(img, cmap='gray')\n",
    "plt.axis('off')\n",
    "cv.imwrite(\"./circle.png\",img )\n"
   ]
  }
